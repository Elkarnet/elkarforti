LDAP_BIND_PASSWORD=bindpassword
LDAP_USER_SEARCH_OU=ou=Users,dc=example,dc=com
LDAP_USER_SEARCH_FILTER=(&(objectClass=*)(samaccountname=%(user)s))
LDAP_GROUP_SEARCH=ou=Groups,dc=example,dc=com
LDAP_REQUIRE_GROUP=cn=FortiAccessGroup,ou=Groups,dc=example,dc=com
LDAP_FLAG_IS_ACTIVE=cn=FortiAccessGroup,ou=Groups,dc=example,dc=com
LDAP_FLAG_IS_STAFF=cn=FortiAccessGroup,ou=Groups,dc=example,dc=com
DB_PATH=/db/db.sqlite3
AUTO_OPEN_CLOSE=1
AUTO_OPEN_HOUR=15
AUTO_OPEN_MIN=0
AUTO_CLOSE_HOUR=7
AUTO_CLOSE_MIN=30
